{
    "version": "2.0.0",
    "tasks": [
        {
            "label": ".NET Build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/league-table.sln",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Docker Build",
            "command": "./docker-compose-install.sh",
            "type": "process"
        },
    ]
}